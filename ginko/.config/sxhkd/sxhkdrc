super + Return
	termite

super + d
	rofi -show drun

super + space
	rofi -show run

super + Tab
	rofi -show window

super + Escape
	pkill -USR1 -x sxhkd

super + alt + Escape
	bspc quit

super + {_,shift +}q
	bspc node -{c,k}

super + m
	bspc desktop -l next

super + {t,s,f}
	bspc node -t{tiled,floating,fullscreen}

super + {_,shift +}{j,k,l,semicolon}
	bspc node -{f,s} {west,south,north,east}

super + bracket{left,right}
	bspc desktop -f {prev,next}
